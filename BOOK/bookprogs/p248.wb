model
{
 for(i in 1:9249)
  {
   for(t in 1:2)
    {
     y[i,t]~dnorm(alpha[i],sigma2inv)       
    }
  alpha[i]~dnorm(mu[i],tau2inv)
  mu[i]<-alpha0 + alpha1*sex[i] 
  }
alpha0~dnorm(0,1.0E-4)
alpha1~dnorm(0,1.0E-4)

sigma2inv~dgamma(.01,.01)
sigma2<-1/sqrt(sigma2inv)

tau2inv~dgamma(.01,.01)
tau2<-1/sqrt(tau2inv)
}